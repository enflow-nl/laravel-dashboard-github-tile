{
    "name": "enflow-nl/spatie-dashboard-github-tile",
    "description": "Spatie dashboard tile using github API",
    "keywords": [
        "enflow",
        "dashboard",
        "tile",
        "github-api"
    ],
    "homepage": "https://github.com/enflow-nl/spatie-dashboard-github-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Bart Noordsij",
            "email": "bnoordsij@enflow.nl"
        }
    ],
    "require": {
        "php": "^7.4",
        "knplabs/github-api": "^2.14",
        "spatie/laravel-dashboard": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Enflow\\Dashboard\\Tile\\Github\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Enflow\\Dashboard\\Tile\\Github\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Enflow\\Dashboard\\Tile\\Github\\GithubServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
